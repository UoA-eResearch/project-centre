<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="ehcache.xsd" updateCheck="true" monitoring="autodetect"
         dynamicConfig="true">

    <cache name="DivisionCache" maxEntriesLocalHeap="100" eternal="false" timeToLiveSeconds="60"
           memoryStoreEvictionPolicy="LFU" transactionalMode="off">
        <persistence strategy="none"/>
    </cache>

    <cache name="DivisionalRoleCache" maxEntriesLocalHeap="100" eternal="false"
           timeToLiveSeconds="60" memoryStoreEvictionPolicy="LFU" transactionalMode="off">
        <persistence strategy="none"/>
    </cache>

    <cache name="PersonStatusCache" maxEntriesLocalHeap="10" eternal="false" timeToLiveSeconds="60"
           memoryStoreEvictionPolicy="LFU" transactionalMode="off">
        <persistence strategy="none"/>
    </cache>

    <cache name="ProjectActionTypeCache" maxEntriesLocalHeap="10" eternal="false"
           timeToLiveSeconds="60" memoryStoreEvictionPolicy="LFU" transactionalMode="off">
        <persistence strategy="none"/>
    </cache>

    <cache name="ProjectStatusCache" maxEntriesLocalHeap="10" eternal="false" timeToLiveSeconds="60"
           memoryStoreEvictionPolicy="LFU" transactionalMode="off">
        <persistence strategy="none"/>
    </cache>

    <cache name="ProjectTypeCache" maxEntriesLocalHeap="10" eternal="false" timeToLiveSeconds="60"
           memoryStoreEvictionPolicy="LFU" transactionalMode="off">
        <persistence strategy="none"/>
    </cache>

    <cache name="ProjectCache" maxEntriesLocalHeap="10" eternal="false" timeToLiveSeconds="60"
           memoryStoreEvictionPolicy="LFU" transactionalMode="off">
        <persistence strategy="none"/>
    </cache>

    <cache name="PersonCache" maxEntriesLocalHeap="10" eternal="false" timeToLiveSeconds="60"
           memoryStoreEvictionPolicy="LFU" transactionalMode="off">
        <persistence strategy="none"/>
    </cache>

</ehcache>

